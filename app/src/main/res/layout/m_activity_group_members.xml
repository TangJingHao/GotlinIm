<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/bg_default"
    android:orientation="vertical"
    tools:context=".view.activity.GroupMembersActivity">

    <include
        android:id="@+id/toolbar_group_members"
        layout="@layout/m_my_toolbar"
        />

    <include
        android:id="@+id/tab_invite_numbers"
        layout="@layout/d_item_user_info_simple"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_member"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>